{
  "name": "@daneroo/ottawajs-lerna-react",
  "version": "1.1.3",
  "description": "A react component which depends on @daneroo/ottawajs-lerna-math",
  "author": "Daniel Lauzon <daniel.lauzon@gmail.com>",
  "homepage": "https://github.com/daneroo/ottawajs-lerna#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/qcic-react.js",
  "umd:main": "dist/qcic-react.umd.js",
  "module": "dist/qcic-react.m.js",
  "source": "src/index.js",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daneroo/ottawajs-lerna.git"
  },
  "bugs": {
    "url": "https://github.com/daneroo/ottawajs-lerna/issues"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "lint": "standard || true",
    "test": "jest --verbose --coverage --silent",
    "test:watch": "jest --watch --runInBand --silent",
    "prebuild": "npm run lint && npm t && rimraf dist",
    "build": "microbundle -o dist/ --sourcemap true --target web --jsx React.createElement",
    "dev": "microbundle watch -o dist/ --sourcemap true --target web --jsx React.createElement --compress false"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.4",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "jest": "^24.8.0",
    "microbundle": "^0.11.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "rimraf": "^2.6.2",
    "standard": "^13.0.0"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "standard": {
    "//parser": "babel-eslint",
    "env": {
      "jest": true
    }
  },
  "dependencies": {
    "@daneroo/ottawajs-lerna-math": "^1.1.3"
  },
